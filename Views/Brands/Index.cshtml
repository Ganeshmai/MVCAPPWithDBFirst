@model IEnumerable<MVCAPPWithDBFirst.Models.Brand>
@{
    ViewBag.Title = "Index";
}

<h2>ALL Brands</h2>
<table class="table">
    <tr>
        <th>Brand Id</th>
        <th>Brand Name</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BrandId</td>
            <td>@item.BrandName</td>
        </tr>
    }
</table>

