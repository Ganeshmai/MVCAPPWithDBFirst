@model IEnumerable<MVCAPPWithDBFirst.Models.Category>
@{
    ViewBag.Title = "Index";
}

<h2>All Categories</h2>

<table class="table">
    <tr>
        <th>Category Id</th>
        <th>Category Name</th>

    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.CategoryId</td>
                    <td>@item.CategoryName</td>
                </tr>
            }
</table>